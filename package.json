{
  "name": "assets-expander",
  "author": "Jakub Pawlowicz <jakub@goalsmashers.com> (http://twitter.com/GoalSmashers)",
  "description": "A well-tested Assets expander - specify assets graph in YAML file and easily expand them into a linear list",
  "keywords": ["css", "javascript", "assets", "yaml"],
  "homepage": "http://github.com/GoalSmashers/assets-expander",
  "repository": {
    "type" : "git",
    "url" : "http://github.com/GoalSmashers/assets-expander.git"
  },
  "version": "0.5.0",
  "main": "index.js",
  "scripts": {
    "check": "jshint .",
    "prepublish": "jshint .",
    "test": "vows"
  },
  "devDependencies": {
    "js-yaml": "3.0.x",
    "jshint": "2.4.x",
    "vows": "0.7.x"
  },
  "jshintConfig": {
    "browser": false,
    "camelcase": true,
    "curly": false,
    "eqeqeq": false,
    "eqnull": true,
    "immed": true,
    "indent": 2,
    "latedef": true,
    "multistr": false,
    "noarg": true,
    "node": true,
    "plusplus": false,
    "quotmark": "single",
    "strict": false,
    "trailing": true,
    "undef": true,
    "unused": true
  },
  "engines": {
    "node": ">=0.8.0"
  }
}
